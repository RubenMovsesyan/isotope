[
    {
        "label": "Run Debug Log",
        "env": {
            "RUST_LOG": "debug"
        },
        "command": "cargo run --bin isotope_test"
    },
    {
        "label": "Run",
        "env": {
            "RUST_LOG": "info"
        },
